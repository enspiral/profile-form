{
    "defaultSeverity": "error",
    "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
    "jsRules": {},
    "rules": {
      "interface-name": false,
      "member-access": [false],
      "jsx-no-lambda": [false],
      "ordered-imports": [false],
      "object-literal-sort-keys": [false],
      "no-console": [false],
      "no-var-requires": true,
      "radix": false
    },
    "rulesDirectory": []
  }